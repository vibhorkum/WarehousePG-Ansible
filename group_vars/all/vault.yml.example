# ============================================================
# Ansible Vault Template for Sensitive Variables
# ============================================================
#
# Setup Instructions:
# 1. Copy this file to vault.yml:
#    cp vault.yml.example vault.yml
#
# 2. Replace placeholder values with actual credentials
#
# 3. Encrypt the file with ansible-vault:
#    ansible-vault encrypt vault.yml
#
# 4. Run playbooks with vault password:
#    ansible-playbook -i inventory.yml playbook.yml --ask-vault-pass
#
# Or use a password file:
#    ansible-playbook -i inventory.yml playbook.yml --vault-password-file ~/.vault_pass
#
# ============================================================

# EDB Repository Token (REQUIRED)
# Get your token from: https://www.enterprisedb.com/repos-downloads
edb_subscription_token: "YOUR_TOKEN_HERE"

# Optional: gpadmin user password
# whpg_gpadmin_password: "YOUR_SECURE_PASSWORD_HERE"
